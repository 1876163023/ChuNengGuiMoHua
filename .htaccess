# .htaccess
<IfModule mod_mime.c>
    # Unity WebGL Build Support
    AddType application/wasm .wasm
    AddType application/javascript .unityweb
    
    # Gzip Compression
    AddEncoding gzip .unityweb
    AddEncoding gzip .js.unityweb
    AddEncoding gzip .wasm.unityweb
    AddEncoding gzip .data.unityweb
    AddEncoding gzip .symbols.json.unityweb
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Handle .unityweb files
    RewriteCond %{REQUEST_URI} \.unityweb$
    RewriteRule ^(.*)$ $1 [L]
    
    # Set proper headers for compressed files
    RewriteCond %{REQUEST_URI} \.unityweb$
    RewriteRule .* - [E=no-gzip:1]
    RewriteRule .* - [E=dont-vary:1]
</IfModule>